#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <main.h>

int main(void)
{
	char *pass = "A34r9n";
	char input[32];

	printf("Enter password: ");
	scanf("%s", input);

	if (strcmp(input, pass) == 0)
	printf("Congratulations!\n");
	else
	printf("Wrong password!\n");

	return (0);
}

